{
    "manifest_version": 2,
    "name": "Email Chrome plugin",
    "description": "This extension connects to an email",
    "version": "1.0",
    "permissions": [
                  "https://gmail.com/",
                  "https://outlook.com/"
    ],
    "browser_action": {
      "default_icon": "logo_plugin.png",
      "default_popup": "popup.html"
    }
  }